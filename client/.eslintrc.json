{
    "extends": [
        "airbnb-base",
        "plugin:vue/essential"
    ],
    "rules": {
        "indent": ["error", 4],
        "object-curly-spacing": ["error", "never"],
        "object-curly-newline": ["error", { "multiline": true }],
        "no-plusplus": "off",
        "func-names": ["error", "never"],
        "import/prefer-default-export": ["error", "never"]
    }
}
